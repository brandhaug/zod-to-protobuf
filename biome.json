{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"formatter": {
		"ignore": ["dist/"]
	},
	"javascript": {
		"formatter": {
			"semicolons": "asNeeded",
			"quoteStyle": "single",
			"trailingCommas": "none"
		}
	},
	"organizeImports": {
		"enabled": true,
		"ignore": ["dist/"]
	},
	"linter": {
		"enabled": true,
		"ignore": ["dist/"],
		"rules": {
			"recommended": true,
			"complexity": {
				"noForEach": "off"
			}
		}
	}
}
