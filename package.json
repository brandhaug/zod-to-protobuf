{
  "name": "zod-to-protobuf",
  "version": "0.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": ["dist/"],
  "targets": {
    "main": {
      "optimize": true
    },
    "module": {
      "optimize": true
    }
  },
  "exports": {
    "import": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "require": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "parcel build src/index.ts --no-source-maps",
    "start": "node dist/index.js",
    "lint": "biome lint",
    "check-write": "biome check --write",
    "typecheck": "tsc",
    "prepare": "husky",
    "test": "vitest",
    "validate": "npm run lint && npm run typecheck && npm run test"
  },
  "keywords": ["zod", "protobuf", "schema", "conversion", "typescript"],
  "author": "Martin Brandhaug",
  "license": "MIT",
  "description": "A utility to convert Zod schemas to Protocol Buffers definitions.",
  "repository": {
    "type": "git",
    "url": "https://github.com/brandhaug/zod-to-protobuf.git"
  },
  "bugs": {
    "url": "https://github.com/brandhaug/zod-to-protobuf/issues"
  },
  "homepage": "https://github.com/brandhaug/zod-to-protobuf#readme",
  "dependencies": {
    "inflection": "3.0.2",
    "zod": "4.0.17"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@parcel/packager-ts": "^2.15.4",
    "@parcel/transformer-typescript-types": "2.15.4",
    "@semantic-release/changelog": "6.0.3",
    "@types/node": "24.2.1",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "parcel": "2.15.4",
    "semantic-release": "24.2.7",
    "tsc-files": "1.1.4",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "*.{ts,json}": ["biome check --write"],
    "*.ts": ["tsc-files"]
  },
  "engines": {
    "node": ">=20"
  }
}
